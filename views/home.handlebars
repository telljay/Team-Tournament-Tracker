<h3>Admin Dashboard</h3>
<table class="table table-hover">
    <thead>
        <th>Tournament Name</th><th>Start Date</th><th>End Date</th><th>Actions</th>
    </thead>
    {{#each everything}}
        <tr>
            <td>{{Name}}</td><td>{{StartDate}}</td><td>{{EndDate}}</td><td><a class="btn btn-outline-success" href="/tournament/{{Id}}">View</a></td>
        </tr>
    {{/each}}
</table>
<a class="btn btn-primary" href="/newTournament">Add New Tournament</a>